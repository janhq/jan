+// Used to notify components within the same tab about auth state changes
+export const AUTH_EVENT_NAME = 'jan-auth-event'
+
+// Auth events
+export const AUTH_EVENTS = {
+  LOGIN: 'auth:login',
+  LOGOUT: 'auth:logout',
+} as const
