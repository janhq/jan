+import { Link, useRouterState, useNavigate } from '@tanstack/react-router'
+import { useLeftPanel } from '@/hooks/useLeftPanel'
+import { useThreadSelection } from '@/hooks/useThreadSelection'
+import { cn } from '@/lib/utils'
+import {
+  IconLayoutSidebar,
