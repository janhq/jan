+/**
+ * Tag Management Dialog
+ * UI for creating and managing project tags
+ */
+
+import { useState, useEffect } from 'react'
+import { tagsService, Tag } from '@/services/tags'
+import {
+  Dialog,
+  DialogContent,
+  DialogDescription,
+  DialogFooter,
+  DialogHeader,
